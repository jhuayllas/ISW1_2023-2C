!City methodsFor: 'as yet unclassified' stamp: 'camila 9/24/2023 17:25:00' prior: 50706765!
hasResidentialZones

		| index |
	zones ifNil: [ ^ false ].

	index := 1.
	
	[index <= zones size] whileTrue: [ | zone |
		zone := zones at: index.
		zone isResidential ifTrue: [ ^ true].	
		index := index + 1.
	].

	^ false.! !

